{
  "uid": "ecommerce-services",
  "title": "Ecommerce Services",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["ecommerce", "spring-boot"],
  "panels": [
    {
      "type": "stat",
      "title": "Order Service Up",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"expr": "up{job=\"order-service\"}", "refId": "A"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false}
      }
    },
    {
      "type": "stat",
      "title": "Inventory Service Up",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"expr": "up{job=\"inventory-service\"}", "refId": "A"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false}
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum by (application) (rate(http_server_requests_seconds_count[1m]))",
          "refId": "A"
        }
      ]
    }
  ]
}
