{
  "uid": "cicd-demo",
  "title": "CI/CD Demo App",
  "schemaVersion": 39,
  "version": 1,
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP Requests / sec",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Uptime (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "process_uptime_seconds",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Memory RSS (bytes)",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS"
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ]
}
