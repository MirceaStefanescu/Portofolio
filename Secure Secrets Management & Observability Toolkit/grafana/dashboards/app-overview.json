{
  "uid": "secrets-toolkit",
  "title": "Secrets Toolkit Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m])) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Vault Secret Fetch Failures",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(vault_secret_fetch_failures_total[5m]))",
          "legendFormat": "failures",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Vault Secret Fetches",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(vault_secret_fetch_total[5m]))",
          "legendFormat": "fetches",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    }
  ]
}
